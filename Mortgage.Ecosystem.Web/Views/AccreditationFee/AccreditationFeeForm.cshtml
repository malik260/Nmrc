@{
    Layout = "~/Views/Shared/_FormWhite.cshtml";
}


<div class="eTicket" id="eTicket">
        <form id="form1" class="form-horizontal m">
            <div class="dialogblockborder dialogblockborderheadline">
                <div class="dialogblock-header" style="height: auto;">
                   
                <div class="form-group">
                    <div class="col-sm-6">
                        <label for="agentName" class="form-label">Agent&nbsp;Name</label>
                        <input id="agentlName" col="AgentName" class="form-control" readonly />

                    </div>

                    <div class="col-sm-6">
                        <label for="phoneNumber" class="form-label">Phone&nbsp;Number</label>
                        <input id="phoneNumber" col="PhoneNumber" class="form-control" readonly />
                    </div>

                </div>

                <div class="form-group">
                    <div class="col-sm-6">
                        <label for="email" class="form-label">Email</label>
                        <input id="email" col="Email" class="form-control" readonly />
                    </div>
                    <div class="col-sm-6">
                        <label for="amountFee" class="form-label">Amount&nbsp;Fee</label>
                        <input id="amountFee" type="number" col="AmountFee" class="form-control" readonly />
                    </div>
                </div>
                    <div class="form-group">
                        <div class="col-sm-6">
                            <label for="paymentOption" class="form-label">Payment Option</label>
                      
                        <div id="paymentOption" col="FactorIndex">  </div>                 
                        </div>
                    </div>
            </div>
            </div>
        </form>
</div>



      <script type="text/javascript">

    $("#paymentOption").cnComboBox({ data: cn.getJson(@Html.Raw(typeof(PaymentOptionEnum).EnumToDictionaryString())) });
      
     </script>
  



