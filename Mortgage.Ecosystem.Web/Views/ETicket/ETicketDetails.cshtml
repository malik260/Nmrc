@{    Layout = "~/Views/Shared/_FormWhite.cshtml";}<div class="container" id="feedback-form-container">    <form id="form" class="form-horizontal m">        <div class="dialogblockborder dialogblockborderheadline">            <div class="dialogblock-header" style="height: auto;">               @* <span class="dialogblockheader">Customer E-Ticket</span>*@                <span class="dialogblockheader">Customer <span id="messageTypePlaceholder"></span></span>                <div class="form-group">                    <div class="col-sm-9">                        <label for="Message" class="control-label">Message</label>                        <textarea id="message" name="Message" class="form-control" style="height:70px" col="Message" readonly></textarea>                    </div>                </div>                <div class="form-group">                    <div class="col-sm-9">                        <label for="ResponseMessage" class="control-label">Response Message (Optional)</label>                        <textarea id="responseMessage" name="ResponseMessage" class="form-control" style="height:70px" col="ResponseMessage"></textarea>                    </div>                </div>                <div class="form-group">                    <div class="col-sm-9">                        <label for="Status" class="control-label">Status<font class="red"> *</font></label>                        <div id="status" col="Status"></div>                    </div>                </div>                           </div>        </div>    </form></div><!-- SweetAlert2 CSS --><link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css"><!-- SweetAlert2 JS --><script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.all.min.js"></script><script type="text/javascript">    var id = cn.request("id");    $(document).ready(function () {        // Initialize form validation        $("#form1").validate();        // Initialize the status combo box        $("#status").cnComboBox({            data: cn.getJson(@Html.Raw(typeof(AdminETicketStatusEnum).EnumToDictionaryString()))        });        if (id > 0) {            $.ajax({                url: '@Url.Content("~/ETicket/GetFormJson")' + '?id=' + id,                type: "get",                contentType: "application/json; charset=utf-8",                success: function (obj) {                    if (obj.Tag == 1) {                        var result = obj.Data;                        $('#message').val(result.Message);                        $('#status').cnComboBox('setValue', result.Status);                        $('#messageType').val(result.MessageType);                        // Set the MessageType into the placeholder                        $('#messageTypePlaceholder').text(result.MessageType);                    }                }            });        }    });    function saveForm(index) {    if ($("#form").validate().form()) {        var postData = $("#form").getWebControls({ Id: id });        postData.ResponseMessage = cn.getLastValue(postData.ResponseMessage);        // Retrieve the selected value from the combo box        postData.Status = $("#status").cnComboBox('getValue');        postData.Status = cn.getLastValue(postData.Status);         if (postData.Status <= 0) {            cn.msgError("Please select E-Ticket Status!");            return false;        }        // Show SweetAlert2 spinner        Swal.fire({            title: 'Processing...',            html: 'Please wait while your request is being processed.',            allowOutsideClick: false,            didOpen: () => {                Swal.showLoading();            }        });        $.ajax({            url: '@Url.Content("~/ETicket/SaveFormDetails")',            type: "post",            data: postData,            success: function (obj) {                // Close SweetAlert2 spinner                Swal.close();                if (obj.Tag == 1) {                    Swal.fire({                        icon: 'success',                        title: 'Success',                        text: obj.Message,                        confirmButtonText: 'OK'                    }).then(() => {                        window.location.reload();                        parent.searchGrid();                        parent.layer.close(index);                    });                } else {                    Swal.fire({                        icon: 'error',                        title: 'Error',                        text: obj.Message,                        confirmButtonText: 'OK'                    });                }            },            error: function (xhr, status, error) {                // Close SweetAlert2 spinner                Swal.close();                Swal.fire({                    icon: 'error',                    title: 'Error',                    text: 'An error occurred while processing your request.',                    confirmButtonText: 'OK'                });            }        });    }}  </script>